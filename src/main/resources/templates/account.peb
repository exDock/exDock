{# @pebvariable name="accounts" type="kotlin.collections.List<com.ex_dock.ex_dock.database.account.FullUser>" #}
{% block title %} Account {% endblock %}

{% block content %}
  <h1>Account</h1>
  <p>Welcome, {{ name }}!</p>
  {% for account in accounts %}
    <div style="display: flex; column-gap: 8px">
        <p>{{ account.component1().component2() }}</p>
        <a href="account/{{ account.component1().component1() }}"><button>View</button></a>
    </div>
{% endfor %}
{% endblock %}
